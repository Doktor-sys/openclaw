# Production Environment Variables
NODE_ENV=production

# Database Configuration
SUPABASE_URL=https://your-supabase-project.supabase.co
SUPABASE_ANON_KEY=your-supabase-anon-key

# Security
JWT_SECRET=CHANGE_THIS_IN_PRODUCTION_USE_LONG_RANDOM_STRING_MIN_32_CHARS

# Server Configuration
PORT=3002

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Session Configuration
SESSION_SECRET=CHANGE_THIS_ANOTHER_LONG_RANDOM_STRING_MIN_32_CHARS
SESSION_MAX_AGE_MS=86400000

# Bot Configuration
BOT_WS_URL=ws://backend:3002
BOT_API_URL=http://backend:3002

# Logging
LOG_LEVEL=info
LOG_FORMAT=json